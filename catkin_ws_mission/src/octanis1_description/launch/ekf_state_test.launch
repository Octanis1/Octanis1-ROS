<launch>
    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se_odom" clear_params="true">
        <rosparam command="load" file="$(find octanis1_description)/params/robot_localization_odom.yaml" />
    </node>


    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se_map" clear_params="true">
        <rosparam command="load" file="$(find octanis1_description)/params/robot_localization_map.yaml" />
        <remap from="odometry/filtered" to="odometry/filtered/global"/>
    </node>

    <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform" clear_params="true">
        <rosparam command="load" file="$(find octanis1_description)/params/navsat_transform.yaml" />
        <remap from="odometry/filtered" to="odometry/filtered/global"/>
        <remap from="gps/fix" to="mavros/global_position/raw/fix" />
        <remap from="imu/data" to="imu2/data" />
    </node>

</launch>
